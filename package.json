{
  "name": "sveltekit-sqlite-drizzle",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "drizzle:migrate && bun ./build/index.js",
    "build": "vite build",
    "dev": "vite dev",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "drizzle:gen": "bun drizzle-kit generate:sqlite",
    "drizzle:migrate": "bun run ./scripts/migrate.ts",
    "drizzle:push": "bun drizzle-k0it push",
    "drizzle:studio": "bun drizzle-kit studio"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/bun": "^1.0.12",
    "autoprefixer": "^10.4.16",
    "drizzle-kit": "^0.20.14",
    "postcss": "^8.4.32",
    "postcss-load-config": "^5.0.2",
    "svelte": "^5.0.0-next.1",
    "svelte-adapter-bun": "^0.5.2",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^3.3.6",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.3"
  },
  "type": "module",
  "dependencies": {
    "@libsql/client": "^0.6.0",
    "bits-ui": "^0.21.2",
    "clsx": "^2.1.0",
    "drizzle-orm": "^0.30.8",
    "mode-watcher": "^0.3.0",
    "nanoid": "^5.0.7",
    "svelte-radix": "^1.1.0",
    "svelte-sonner": "^0.3.22",
    "tailwind-merge": "^2.2.2",
    "tailwind-variants": "^0.2.1"
  }
}
